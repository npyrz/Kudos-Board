generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Boards {
  id            Int      @id @default(autoincrement())
  category      String
  title         String
  author        String?
}

model Cards {
  id            Int      @id @default(autoincrement())
  title         String
  description   String
  img           String
  owner         String?
  upvote        Int
  boardId       Int
}